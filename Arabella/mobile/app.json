{
  "expo": {
    "name": "arabella-mobile",
    "description": "Arabella react native project.",
    "slug": "arabella-mobile",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "pl.arabella",
      "supportsTablet": true
    },
    "android": {
      "package": "pl.arabella"
    }
  }
}
